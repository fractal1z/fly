        <param name="xmax2" type="double" value="-3.0"/>
        <param name="ymax2" type="double" value="3.0"/>
        <param name="zmax2" type="double" value="4.0"/>
        <param name="xmin2" type="double" value="-6.0"/>
        <param name="ymin2" type="double" value="-4.0"/>
